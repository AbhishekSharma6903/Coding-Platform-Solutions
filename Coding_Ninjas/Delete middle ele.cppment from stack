#include <bits/stdc++.h> 

void deleteMiddle(stack<int>&inputStack, int N){

   int size = inputStack.size();

   stack<int> stk;

   int count = 0;

 

   if(size%2 == 0){

      while(count<(size/2)-1){

         stk.push(inputStack.top());

         inputStack.pop();

         count++;

      }

   }else{

      while(count<size/2){

         stk.push(inputStack.top());

         inputStack.pop();

         count++;

      }

   }

 

   inputStack.pop();

 

   while(!stk.empty()){

      inputStack.push(stk.top());

      stk.pop();

   }

}yuj
